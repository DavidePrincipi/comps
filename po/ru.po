#: ../nethserver-groups.xml.in.h:1
msgid "Build system"
msgstr "Система сборки"

#: ../nethserver-groups.xml.in.h:2
msgid "Tools for building RPMs"
msgstr "Инструменты для сборки пакетов RPM"

#: ../nethserver-groups.xml.in.h:3
msgid "Additional ISO packages"
msgstr "Дополнительные пакеты ISO"

#: ../nethserver-groups.xml.in.h:4
msgid "Backup"
msgstr "Резервное копирование"

#: ../nethserver-groups.xml.in.h:5
msgid "Backup of configuration and data"
msgstr "Резервное копирование настроек и данных"

#: ../nethserver-groups.xml.in.h:6
msgid "DNS and DHCP server"
msgstr "Серверы DNS и DHCP"

#: ../nethserver-groups.xml.in.h:7
msgid "Daemons and tools for DHCP and DNS server"
msgstr "Демоны и инструменты DHCP и DNS серверов"

#: ../nethserver-groups.xml.in.h:8
msgid "Fax server"
msgstr "Факс сервер"

#: ../nethserver-groups.xml.in.h:9
msgid "Configure HylaFax+ and manage IAX modems"
msgstr "Настройка HylaFax+ и модемов IAX"

#: ../nethserver-groups.xml.in.h:10
msgid "Web-based fax client"
msgstr "Web-клиент факса"

#: ../nethserver-groups.xml.in.h:11
msgid "Manage faxes from a simple web interface"
msgstr "Управление факсами через простой web интерфейс"

#: ../nethserver-groups.xml.in.h:12
msgid "Basic firewall"
msgstr "Базовый межсетевой экран"

#: ../nethserver-groups.xml.in.h:13
msgid "Configure network adapters and basic firewall"
msgstr "Настройка сетевых интерфейсов и базового экрана"

#: ../nethserver-groups.xml.in.h:14
msgid "File server"
msgstr "Файловый сервер"

#: ../nethserver-groups.xml.in.h:15
msgid "Daemons and tools for network file sharing"
msgstr "Демоны и утилиты для доступа к файлам из сети"

#: ../nethserver-groups.xml.in.h:16
msgid "FTP server"
msgstr "Сервер FTP"

#: ../nethserver-groups.xml.in.h:17
msgid "Configure the FTP server (vsftpd)"
msgstr "Настройка сервера FTP (vsftpd)"

#: ../nethserver-groups.xml.in.h:18
msgid "Groupware"
msgstr "Сервера совместной работы"

#: ../nethserver-groups.xml.in.h:19
msgid "SOGo server and Thundebird extensions"
msgstr "Сервер SOGo и расширения Thundebird"

#: ../nethserver-groups.xml.in.h:20
msgid "Email"
msgstr "Электронная почта"

#: ../nethserver-groups.xml.in.h:21
msgid "Email server and filter"
msgstr "Сервер эл.почты и фильтры"

#: ../nethserver-groups.xml.in.h:22
msgid "SMTP proxy"
msgstr "Прокси SMTP"

#: ../nethserver-groups.xml.in.h:23
msgid "POP3 connector"
msgstr "POP3 connector"

#: ../nethserver-groups.xml.in.h:24
msgid "Fetch messages from external email accounts with POP3 or IMAP access"
msgstr "Загрузка сообщений из внешних серверов эл.почты через POP3 или IMAP"

#: ../nethserver-groups.xml.in.h:25
msgid "Instant messaging"
msgstr "Сообщения"

