  <group>
    <id>nethserver-dns-dhcp</id>
    <_name>DNS and DHCP server</_name>
    <_description>Daemons and tools for DHCP and DNS server</_description>
    <default>false</default>
    <uservisible>true</uservisible>
    <packagelist>
      <packagereq type="mandatory">nethserver-dnsmasq</packagereq>
    </packagelist>
  </group>
  <group>
    <id>nethserver-fax-server</id>
    <_name>Fax server</_name>
    <_description>Configure HylaFax+ and manage IAX modems</_description>
    <default>false</default>
    <uservisible>true</uservisible>
    <packagelist>
      <packagereq type="mandatory">nethserver-hylafax</packagereq>
      <packagereq type="mandatory">nethserver-iaxmodem</packagereq>
    </packagelist>
  </group>
  <group>
    <id>nethserver-firewall-base</id>
    <_name>Basic firewall</_name>
    <_description>Configure network adapters and basic firewall</_description>
    <default>false</default>
    <uservisible>true</uservisible>
    <packagelist>
      <packagereq type="mandatory">nethserver-firewall-base</packagereq>
    </packagelist>
  </group>
  <group>
    <id>nethserver-file-server</id>
    <_name>File server</_name>
