      <packagereq type="mandatory">nethserver-squidguard</packagereq>
      <packagereq type="mandatory">nethserver-squidclamav</packagereq>
      <packagereq type="optional">nethserver-lightsquid</packagereq>
    </packagelist>
  </group>

