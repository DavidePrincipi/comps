    <uservisible>true</uservisible>
    <packagelist>
      <packagereq type="mandatory">nethserver-sogo</packagereq>
      <packagereq type="optional">nethserver-sogo-thunderbird</packagereq>
    </packagelist>
  </group>
  <group>
    <id>nethserver-mail</id>
    <_name>Email</_name>
    <_description>Email server and filter</_description>
    <default>false</default>
    <uservisible>true</uservisible>
