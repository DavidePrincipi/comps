      <packagereq type="mandatory">nethserver-squid</packagereq>
      <packagereq type="mandatory">nethserver-dnsmasq</packagereq>
      <packagereq type="mandatory">nethserver-httpd</packagereq>
      <packagereq type="optional">nethserver-lightsquid</packagereq>
    </packagelist>
  </group>

  <group>
    <id>nethserver-web-filter</id>
    <_name>Web filter</_name>
