      <packagereq type="mandatory">nethserver-mail-filter</packagereq>
      <packagereq type="mandatory">nethserver-mail-server</packagereq>
      <packagereq type="mandatory">nethserver-samba</packagereq>
    </packagelist>
  </group>
  <group>
    <id>nethserver-messaging</id>
