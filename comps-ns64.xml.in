    <default>false</default>
    <uservisible>true</uservisible>
    <packagelist>
      <packagereq type="mandatory">nethserver-httpd</packagereq>
      <packagereq type="mandatory">nethserver-ibays</packagereq>
      <packagereq type="mandatory">nethserver-samba</packagereq>
    </packagelist>
  </group>
  <group>
    <id>nethserver-groupware</id>
    <_name>Groupware</_name>
