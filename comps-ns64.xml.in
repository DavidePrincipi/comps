  <group>
    <id>nethserver-nut</id>
    <_name>UPS support</_name>
    <_description>UPS management and monitoring configuration</_description>
    <default>false</default>
    <uservisible>true</uservisible>
    <packagelist>
      <packagereq type="mandatory">nethserver-nut</packagereq>
      <packagereq type="optional">nethserver-collectd</packagereq>
      <packagereq type="optional">collectd-nut</packagereq>
    </packagelist>
  </group>
  <group>
    <id>nethserver-statistics</id>
    <_name>Statistics</_name>
    <_description>Collect and analyse system statistics</_description>
    <default>false</default>
    <uservisible>true</uservisible>
    <packagelist>
      <packagereq type="mandatory">nethserver-collectd</packagereq>
      <packagereq type="optional">nethserver-collectd-web</packagereq>
    </packagelist>
  </group>

  <group>
    <id>nethserver-web-proxy</id>
    <_name>Web proxy</_name>
    <_description>Squid web caching proxy configuration</_description>
    <default>true</default>
    <uservisible>true</uservisible>
